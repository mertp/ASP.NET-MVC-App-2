<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Birds'Club</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Styles/project.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/gallery.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/about_us.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/contact.css")" rel="stylesheet" type="text/css" />
    @RenderSection("Styles", false)

</head>
<body>
   
    <div >
        @RenderBody()
        
        
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
